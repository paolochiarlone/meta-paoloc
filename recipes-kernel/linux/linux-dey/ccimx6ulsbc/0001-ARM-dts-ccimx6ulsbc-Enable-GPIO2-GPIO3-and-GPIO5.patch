From e9198d8208bd6434028980ce8b025ef482a12d49 Mon Sep 17 00:00:00 2001
From: Cameron Haegle <cameronh@digi.com>
Date: Tue, 8 May 2018 14:44:55 -0500
Subject: [PATCH] ARM dts ccimx6ulsbc Enable GPIO2, GPIO3 and GPIO5

---
 arch/arm/boot/dts/imx6ul-ccimx6ulsbc.dtsi | 10 +++++++++-
 1 file changed, 9 insertions(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/imx6ul-ccimx6ulsbc.dtsi b/arch/arm/boot/dts/imx6ul-ccimx6ulsbc.dtsi
index cef5088..894182a 100644
--- a/arch/arm/boot/dts/imx6ul-ccimx6ulsbc.dtsi
+++ b/arch/arm/boot/dts/imx6ul-ccimx6ulsbc.dtsi
@@ -257,7 +257,7 @@
 		     &pinctrl_lcdif_clken
 		     &pinctrl_lcdif_hvsync>;
 	lcd-supply = <&ldo4_ext>;       /* BU90T82 LVDS bridge power */
-	display = <&g101evn010>;
+	display = <&f07a0102>;
 	status = "disabled";
 
 	f07a0102: display@0 {
@@ -715,6 +715,14 @@
 
 		/* General purpose pinctrl */
 		pinctrl_hog: hoggrp {
+                    fsl,pins = <
+                        /* EXP_GPIO_1 */
+                        MX6UL_PAD_GPIO1_IO05__GPIO1_IO05                0x10b0
+                        /* EXP_GPIO_2 */
+                        MX6UL_PAD_GPIO1_IO03__GPIO1_IO03                0x10b0
+                        /* EXP_GPIO_3 */
+                        MX6UL_PAD_GPIO1_IO02__GPIO1_IO02                0x10b0
+                    >;
 		};
 	};
 };
